    "jspdf": "^3.0.4",
    "pdfmake": "^0.2.20",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/file-saver": "^2.0.7",
    "@types/html-to-pdfmake": "^2.4.4",
    "@types/pdfmake": "^0.2.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^39.2.4",
